class SwitchObj: 
    
    def __init__(self): 
        ID = ''
        portId = ""
        Description= ""
        vlan = []
        username = ""
        password = ""
        subId = ""
        cookies = ""
        
        
    def setID(self,switchID):
        self.ID=switchID
        return
    
    def setPort(self,portID):
        self.portId = portID
        return
    
    def setDescription(self,description):
        self.Description = description
        return
    
    def setVlan(self,arr=[]):
        self.vlan = arr
        print("vlan"
        return
        
   
